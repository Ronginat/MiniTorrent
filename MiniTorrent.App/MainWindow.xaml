<Window x:Class="MiniTorrent.App.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MiniTorrent.App"
        mc:Ignorable="d"
        Title="LogInWindow" Height="350" Width="525">
    <Grid>
        <Label x:Name="UsernameLabel" Content="Username" HorizontalAlignment="Left" Margin="113,74,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="UsernameTextBox" HorizontalAlignment="Left" Height="23" Margin="113,105,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="232" TabIndex="0"/>
        <Label x:Name="PasswordLabel" Content="Password" HorizontalAlignment="Left" Margin="113,142,0,0" VerticalAlignment="Top"/>
        <PasswordBox x:Name="PasswordBox" HorizontalAlignment="Left" Margin="113,173,0,0" VerticalAlignment="Top" Width="232" Height="23" TabIndex="1"/>
        <Button x:Name="LoginButton" Content="LogIn" HorizontalAlignment="Left" Margin="192,220,0,0" VerticalAlignment="Top" Width="78" Click="LoginButton_Click" TabIndex="2"/>

    </Grid>
</Window>
